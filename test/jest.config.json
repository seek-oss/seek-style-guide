{
  "testPathIgnorePatterns": [
    "<rootDir>[/\\\\](dist|node_modules)[/\\\\]"
  ],
  "moduleNameMapper": {
    "\\.(css|less)$": "identity-obj-proxy"
  },
  "transform": {
    "^.+\\.js$": "<rootDir>/test/babel.transform.js",
    "^.+\\.svg$": "<rootDir>/test/svg.transform.js"
  },
  "snapshotSerializers": [
    "<rootDir>/node_modules/enzyme-to-json/serializer"
  ]
}
