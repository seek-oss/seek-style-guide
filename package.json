{
  "name": "seek-style-guide",
  "version": "2.0.0",
  "description": "Living style guide containing the building blocks and design principles for SEEK web applications",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "lint": "eslint app react ./*.js",
    "prebuild": "npm run lint",
    "local-build": "rm -rf dist/ && mkdir dist && NODE_ENV=production webpack --config webpack.gh-pages.server.config.js && rm dist/render.js && NODE_ENV=production webpack --config webpack.gh-pages.client.config.js",
    "gh-pages-build": "rm -rf dist/ && mkdir dist && NODE_ENV=production BASE_HREF=/seek-style-guide/ webpack --config webpack.gh-pages.server.config.js && rm dist/render.js && NODE_ENV=production webpack --config webpack.gh-pages.client.config.js",
    "preversion": "npm run gh-pages-build",
    "postversion": "git push && git push --tags && npm run deploy",
    "deploy": "gh-pages -d dist",
    "static": "opn http://localhost:1804 && superstatic dist --port 1804",
    "install-git-hooks": "./scripts/install-git-hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SEEK-Jobs/seek-style-guide.git"
  },
  "author": "SEEK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SEEK-Jobs/seek-style-guide/issues"
  },
  "homepage": "https://github.com/SEEK-Jobs/seek-style-guide#readme",
  "dependencies": {
    "autoprefixer": "6.3.6",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "basekick": "2.0.1",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "postcss-loader": "0.9.1",
    "raw-loader": "0.5.1",
    "style-loader": "0.13.1",
    "svgo": "0.6.6",
    "svgo-loader": "1.1.0",
    "url-loader": "0.5.7"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "ejs": "^2.4.1",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "gh-pages": "^0.11.0",
    "hex-rgb": "^1.0.0",
    "json-loader": "^0.5.4",
    "jsx-to-string": "^0.2.11",
    "less-vars-to-js": "^1.1.1",
    "lodash.debounce": "^4.0.6",
    "opn": "^4.0.1",
    "opn-cli": "^3.1.0",
    "react": "^15.0.2",
    "react-addons-pure-render-mixin": "^15.0.2",
    "react-baseline": "^1.0.1",
    "react-copy-to-clipboard": "4.1.0",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-prism": "^3.2.0",
    "react-router": "^2.4.0",
    "static-site-generator-webpack-plugin": "^2.0.1",
    "superstatic": "^4.0.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
