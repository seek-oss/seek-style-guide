{
  "name": "seek-asia-style-guide",
  "version": "0.0.0-development",
  "description": "Living style guide containing the building blocks and design principles for SEEK web applications",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "BASE_HREF=/ node docs/server",
    "start-in-window": "node docs/server",
    "start-header-footer": "webpack-dev-server --config standalone/header-footer/webpack.config.js --content-base dist",
    "test": "npm run test-unit && npm run lint && npm run build",
    "test-unit": "BABEL_ENV=test jest --config ./test/jest.config.json",
    "test-in-window": "npm run test-unit-in-window && npm run lint && npm run build",
    "test-unit-in-window": "jest --config ./test/jest.config.json",
    "update-snapshot": "npm run test-unit -- --updateSnapshot",
    "lint": "npm run lint-js && npm run lint-less",
    "lint-js": "eslint app standalone/*/src docs/src react test",
    "lint-less": "stylelint '{theme,react,standalone,docs}/**/*.less'",
    "clean": "rm -rf dist && rm -rf docs/dist && mkdir -p dist && mkdir -p docs/dist",
    "compile": "npm run compile-render && npm run compile-client && npm run compile-header-footer",
    "compile-render": "NODE_ENV=production BASE_HREF=${BASE_HREF:-/} webpack --config docs/webpack.static.render.config.js && rm docs/dist/render.js",
    "compile-client": "NODE_ENV=production BASE_HREF=${BASE_HREF:-/} webpack --config docs/webpack.static.client.config.js",
    "compile-header-footer": "NODE_ENV=production webpack --config standalone/header-footer/webpack.config.js",
    "build": "npm run clean && npm run compile",
    "build-gh-pages": "BASE_HREF=/seek-asia-style-guide/ npm run build",
    "static": "opn http://localhost:1804 && superstatic docs/dist --port 1804",
    "deploy-gh-pages": "node scripts/deploy-gh-pages.js",
    "deploy-surge": "surge -p ./docs/dist",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "sketch-to-git": "node scripts/sketch-to-git",
    "git-to-sketch": "node scripts/git-to-sketch"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seekinternational/seek-asia-style-guide.git"
  },
  "author": "SEEK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seekinternational/seek-asia-style-guide/issues"
  },
  "homepage": "https://github.com/seekinternational/seek-asia-style-guide#readme",
  "dependencies": {
    "autoprefixer": "6.7.7",
    "autosuggest-highlight": "^3.1.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-react-displayname": "^0.0.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "basekick": "3.0.0",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "command-line-args": "^4.0.6",
    "css-in-js-loader": "^0.1.2",
    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "lodash": "4.17.4",
    "pad-left": "^2.1.0",
    "postcss-js": "^1.0.0",
    "postcss-loader": "0.9.1",
    "prop-types": "^15.6.0",
    "raw-loader": "0.5.1",
    "react-autosuggest": "9.3.1",
    "react-isolated-scroll": "^0.1.0",
    "react-scrolllock": "1.0.8",
    "style-loader": "0.13.1",
    "svgo": "0.7.1",
    "svgo-loader": "1.1.0",
    "uglify-loader": "1.3.0",
    "url-loader": "0.6.0",
    "webpack-node-externals": "1.2.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-helmet": "^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-jest": "^18.0.0",
    "chai": "^3.5.0",
    "commitizen": "^2.9.5",
    "condition-circle": "^1.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "ejs": "^2.4.2",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "enzyme-to-json": "^3.0.0",
    "es6-promisify": "^5.0.0",
    "eslint": "^3.17.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-jest": "^1.0.2",
    "eslint-plugin-react": "^6.0.0-alpha.1",
    "extract-zip": "^1.6.5",
    "gh-pages": "^0.11.0",
    "glob": "^7.1.1",
    "hex-rgb": "^1.0.0",
    "history": "^3.3.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.12.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.0",
    "json-loader": "^0.5.4",
    "jsx-to-string": "^0.2.13",
    "less-vars-to-js": "^1.1.1",
    "opn": "^4.0.1",
    "opn-cli": "^3.1.0",
    "postcss-prefix-selector": "^1.6.0",
    "raf": "^3.3.2",
    "react": "^16.0.0",
    "react-addons-pure-render-mixin": "^15.3.1",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^16.0.0",
    "react-element-to-jsx-string": "^12.0.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-router": "^3.2.0",
    "react-shallow-testutils": "^3.0.0",
    "react-test-renderer": "^16.0.0",
    "rewrite-files": "^1.0.1",
    "rimraf": "^2.6.1",
    "semantic-release": "^7.0.2",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "string-replace-loader": "^1.2.0",
    "stylelint": "8.0.0",
    "stylelint-config-css-modules": "^1.0.0",
    "stylelint-config-standard": "^16.0.0",
    "superstatic": "^4.0.3",
    "surge": "^0.18.0",
    "validate-commit-msg": "^2.8.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-fail-plugin": "^1.0.6",
    "zip-folder": "^1.0.0"
  },
  "release": {
    "verifyConditions": "condition-circle"
  }
}
