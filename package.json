{
  "name": "seek-style-guide",
  "version": "8.0.0",
  "description": "Living style guide containing the building blocks and design principles for SEEK web applications",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "BASE_HREF=/ node server",
    "lint": "eslint app react test ./*.js",
    "test": "BABEL_ENV=test mocha 'react/**/*.test.js' --compilers js:babel-register --require test/setup.js --harmony-proxies",
    "prebuild": "npm run lint && npm test",
    "local-build": "npm run prebuild && rm -rf dist/ && mkdir dist && NODE_ENV=production BASE_HREF=/ webpack --config webpack.gh-pages.server.config.js && rm dist/render.js && NODE_ENV=production webpack --config webpack.gh-pages.client.config.js",
    "gh-pages-build": "npm run prebuild && rm -rf dist/ && mkdir dist && NODE_ENV=production BASE_HREF=/seek-style-guide/ webpack --config webpack.gh-pages.server.config.js && rm dist/render.js && NODE_ENV=production webpack --config webpack.gh-pages.client.config.js",
    "preversion": "npm run gh-pages-build",
    "postversion": "git push && git push --tags && npm run deploy",
    "deploy": "gh-pages -d dist && printf '\nURL: http://seek-jobs.github.io/seek-style-guide\n\n'",
    "static": "opn http://localhost:1804 && superstatic dist --port 1804",
    "install-git-hooks": "./scripts/install-git-hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SEEK-Jobs/seek-style-guide.git"
  },
  "author": "SEEK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SEEK-Jobs/seek-style-guide/issues"
  },
  "homepage": "https://github.com/SEEK-Jobs/seek-style-guide#readme",
  "dependencies": {
    "autoprefixer": "6.3.7",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.10.2",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "base64-font-loader": "0.0.4",
    "basekick": "2.0.1",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "css-loader": "0.25.0",
    "emoji-dictionary": "1.0.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "postcss-loader": "0.9.1",
    "raw-loader": "0.5.1",
    "style-loader": "0.13.1",
    "svgo": "0.6.6",
    "svgo-loader": "1.1.0",
    "uglify-loader": "1.3.0",
    "url-loader": "0.5.7",
    "webpack-node-externals": "1.2.0"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "ejs": "^2.4.2",
    "eslint": "^3.1.0",
    "eslint-plugin-mocha": "^4.0.0",
    "eslint-plugin-react": "^6.0.0-alpha.1",
    "gh-pages": "^0.11.0",
    "glob": "^7.0.5",
    "hex-rgb": "^1.0.0",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "jsx-to-string": "^0.2.13",
    "less-vars-to-js": "^1.1.1",
    "lodash.debounce": "^4.0.6",
    "mocha": "^2.5.3",
    "mock-css-modules": "^1.0.0",
    "opn": "^4.0.1",
    "opn-cli": "^3.1.0",
    "prunk": "^1.2.1",
    "react": "^15.2.1",
    "react-addons-pure-render-mixin": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-autosuggest": "^3.8.0",
    "react-baseline": "^1.0.1",
    "react-copy-to-clipboard": "4.2.1",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-router": "^2.5.2",
    "react-shallow-testutils": "^2.0.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "static-site-generator-webpack-plugin": "^2.1.0",
    "superstatic": "^4.0.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
